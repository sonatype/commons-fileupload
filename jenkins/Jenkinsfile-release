library('private-pipeline-library')
library('jenkins-shared')

mavenReleasePipeline(usePublicSettingsXmlFile: false,
        onSuccess: { build, env ->
            notifyChat(env: env, currentBuild: build, room: 'apacheupload-test')
        },
        onFailure: { build, env ->
            notifyChat(env: env, currentBuild: build, room: 'apacheupload-test')
        },
        deployBranch: 'sonatype-release'
)
